{
    "name": "Hello World Extension", // 插件名称
    "version": "0.0.1", // 插件版本
    "manifest_version": 2, //清单文件版本，必须写，且必须为2
    "description":"Chrome Extension Demo",    
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "chrome_url_overrides": {
        "newtab": "tab.html"
    },
    "browser_action":{
        "default_icon":"imgs/icons/Sogrey.png",
        "default_title":"My Demo",
        "default_popup":"popup.html"
    }
}

