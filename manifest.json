{
    "name": "Hello World Extension", 
    "version": "0.0.1", 
    "manifest_version": 2, 
    "description":"Chrome Extension Demo",    
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "chrome_url_overrides": {
        "newtab": "tab.html"
    },
    "browser_action":{
        "default_icon":"imgs/icons/Sogrey.png",
        "default_title":"My Demo",
        "default_popup":"popup.html"
    },
    "options_page":"options.html",
    "permissions":[
        "storage",
        "notifications",
        "contextMenus",
        "tabs"
    ]
}

